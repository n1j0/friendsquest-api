  "version": "0.0.1",
  "description": "API for FriendsQuest-APP",
  "main": "./.out/index.js",
  "scripts": {
    "dev": "nodemon --ext ts --loader ts-node/esm --experimental-specifier-resolution node ./src --require dotenv/config dotenv_config_path=../.env",
